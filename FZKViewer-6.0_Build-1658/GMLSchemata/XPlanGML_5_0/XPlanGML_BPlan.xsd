      <extension base="xplan:BP_UeberlagerungsobjektType">
        <sequence>
          <element name="DNmin" minOccurs="0" type="gml:AngleType">
            <annotation>
              <documentation>Minimal zulässige Dachneigung bei einer Bereichsangabe.</documentation>
            </annotation>
          </element>
          <element name="DNmax" minOccurs="0" type="gml:AngleType">
            <annotation>
              <documentation>Maximal zulässige Dachneigung bei einer Bereichsangabe.</documentation>
            </annotation>
          </element>
          <element name="DN" minOccurs="0" type="gml:AngleType">
