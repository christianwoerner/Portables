  <!--LP_Status-->
  <!--=========================================================================================-->
  <simpleType name="LP_Status">
    <restriction base="string">
      <enumeration value="1000">
        <!--Festsetzung-->
        <annotation>
          <documentation>Festsetzung im Landschaftsplan</documentation>
        </annotation>
      </enumeration>
      <enumeration value="2000">
        <!--Geplant-->
